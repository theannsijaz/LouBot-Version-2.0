<!-- Original pattern -->
<category>
  <pattern>WHO IS * OF *</pattern>
  <template>
    <think>
      <set name="relationx"><star/></set>
      <set name="namex"><star index="2"/></set>
    </think>
    <get name="namey"/> is <get name="relationx"/> of <get name="namex"/>.
  </template>
</category>

